<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="report_kostan_order">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="iditakostan.report_idita_kostan_order"/>
                </t>
            </t>
        </template>

        <record id="action_report_wo_order" model="ir.actions.report">
            <field name="name">Report Penyewa Kamar Kostan</field>
            <field name="model">idita.order_kostan</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">iditakostan.report_kostan_order</field>
            <field name="report_file">iditakostan.report_kostan_order</field>
            <field name="binding_model_id" ref="model_idita_order_kostan"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>